config qbittorrent 'main'
	option Locale 'zh'
	option BTProtocol 'Both'
	option DisableAutoTMMByDefault 'true'
	option CategoryChanged 'false'
	option DefaultSavePathChanged 'true'
	option CategorySavePathChanged 'true'
	option Encryption '0'
	option MaxRatioAction '0'
	option SeedChokingAlgorithm 'FastestUpload'
	option Enabled 'false'
	option user 'nobody'
	option profile '/etc/config'
	option SavePath '/mnt/sda1'
	option Port '9090'
	option AutoDeleteAddedTorrentFile 'IfAdded'
	option TempPath '/mnt/sda1/temp'
	option CreateTorrentSubfolder 'false'
	option UseUPnP 'true'
	option HostHeaderValidation 'false'
